   </license>

   <requires>
      <import plugin="ch.elexis.core" version="3.2.0" match="greaterOrEqual"/>
      <import plugin="ch.rgw.utility" version="3.2.0" match="greaterOrEqual"/>
      <import plugin="ch.elexis.core.data" version="3.2.0" match="greaterOrEqual"/>
      <import plugin="ch.elexis.core.ui" version="3.2.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui" version="3.8.2" match="greaterOrEqual"/>
      <import plugin="ch.elexis.core.ui.icons" version="3.2.0" match="greaterOrEqual"/>
      <import plugin="ch.elexis.omnivore" version="3.0.0" match="greaterOrEqual"/>
